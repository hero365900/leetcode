#include <iostream>

#include "../util/aux_func.h"

using namespace std;

int removeDuplicates(int A[], int n) {
  if (n == 0 || n == 1) return n;
  int next(0), i(0), start(i + 1);
  while (i != n) {
    start = i + 1;
    while (start != n && A[start] == A[i]) {
      ++start;
    }
    if (next != i) {
      A[next] = A[i];
      ++next;
      if (start - i > 1) {
	A[next] = A[i];
	++next;
      }
    } else {
      ++next;
      if (start - i > 1) ++next;
    }
    i = start;
  }
  return next;
}

int main() {
  int a[100];
  int n(0);
  string line;
  while (cin >> line) {
    n = loadArray(line, a);
    cout << removeDuplicates(a, n) << endl;
  }
  return 0;
}
